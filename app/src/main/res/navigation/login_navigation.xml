<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_navigation"
    app:startDestination="@id/phoneAuthFragment">

    <fragment
        android:id="@+id/phoneAuthFragment"
        android:name="com.conamobile.cutecut.fragment.PhoneAuthFragment"
        android:label="fragment_phone_auth"
        tools:layout="@layout/fragment_phone_auth">
        <action
            android:id="@+id/action_phoneAuthFragment_to_nameAuthFragment"
            app:destination="@id/nameAuthFragment" />
    </fragment>

    <fragment
        android:id="@+id/nameAuthFragment"
        android:name="com.conamobile.cutecut.fragment.NameAuthFragment"
        android:label="fragment_name_auth"
        tools:layout="@layout/fragment_name_auth" />
</navigation>